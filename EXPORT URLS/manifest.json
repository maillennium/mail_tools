{
  "manifest_version": 2,
  "name": "Extract URLs",
  "version": "1.0",
  "description": "Extracts URLs from emails in a folder",
  "applications": {
    "gecko": {
      "id": "extract-urls@example.com",
      "strict_min_version": "68.0"
    }
  },
  "permissions": [
    "accountsRead",
    "messagesRead",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png",
      "48": "icons/icon-48.png",
      "64": "icons/icon-64.png"
    }
  }
}
